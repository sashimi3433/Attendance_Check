{
  "builds": [
    {
      "src": "myapp/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myapp/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "myapp.settings"
  }
}